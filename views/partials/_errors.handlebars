{{#each userError}}
<div class="row">
    <div class="col-md-6 mx-auto">
        <div class="alert alert-danger">
            {{text}}
        </div>
    </div>
</div>
{{else}}
{{/each}}

{{#if error}}
<div class="row">
    <div class="col-md-6 mx-auto">
        <div class="alert alert-danger">
            {{error}}
        </div>
    </div>
</div>

{{/if}}